pipeline{
    agent { docker {image 'node:14'
            args '-u root:sudo'}} 

    stages{
        stage('Dep install'){
            steps{
              //  sh 'sudo chown -R 995:993 "/.nmp"'
                sh 'npm install'
            }
        }
        stage('package'){
            steps{
                sh 'npm pack'
            }
        }
    }
}